Source Code:

package Exp_5;

public class Student {
	private int rollNo;
	private String name;
	private int year;
	private double cgpa;
	
	public Student(int rollNo, String name,int year, double cgpa) {
		super();
		this.rollNo = rollNo;
		this.name = name;
		this.year = year;
		this.cgpa = cgpa;
	}

	public int getRollNo() {
		return rollNo;
	}

	public String getName() {
		return name;
	}

	public int getYear() {
		return year;
	}


	public double getCgpa() {
		return cgpa;
	}

	public void setYear(int year) {
		this.year = year;
	}
	
	public void setCgpa(double cgpa) {
		this.cgpa = cgpa;
	}

	@Override
	public String toString() {
		return "Student [rollNo=" + rollNo + ", name=" + name + ", year=" + year + ", cgpa=" + cgpa + "]";
	}
}

//Main Class

package Exp_5;
import java.util.*;

class sortByName implements Comparator<Student> {

        public int compare(Student s1,Student s2){
                return s1.getName().compareTo(s2.getName());
        }

}

class sortByCgpa implements Comparator<Student> {

        public int compare(Student s1,Student s2){
                return Double.compare(s2.getCgpa(), s1.getCgpa());
        }
}

public class TestStudent {

        public static void main(String[] args) {
        	Scanner sc = new Scanner(System.in);
             Set<Student> students = new LinkedHashSet<>();

             students.add(new Student(101, "Gurumoorthy", 2, 9.9));
             students.add(new Student(102, "Tanjiro", 3, 7.5));
             students.add(new Student(103, "Faali", 2, 9.1));
             students.add(new Student(104, "Naruto", 1, 6.8));
             students.add(new Student(105, "Kailash", 3, 9.5));
             students.add(new Student(106, "Sasuke", 2, 8.2));
             students.add(new Student(101, "Naruto", 2, 8.9));//duplicate

             System.out.println("*****All Students*****\n");
             for (Student s : students) {
                 System.out.println(s);
             }

             //Sort the Students based on names
             List <Student> listByName = new ArrayList<>(students);
             Collections.sort(listByName, new sortByName());

             System.out.println("\n*****After sorting Alphabetically***** \n");
             for (Student s : listByName) {
                 System.out.println(s);
             }

             //To display the TOP three Students by CGPA

             List <Student> listByCgpa = new ArrayList<>(students);
             Collections.sort(listByCgpa, new sortByCgpa());
             System.out.println();
             System.out.println("\n*****To 3 Students based on CGPA*****\n ");
             for(int i=0;i<3;i++){
                     System.out.println(listByCgpa.get(i));
             }

             //To display the particular year
             System.out.println();
             System.out.print("\nEnter the Year to be Search: ");
             int targetYear = sc.nextInt();
             System.out.println("\n------Display the "+targetYear+ " year Students------\n ");
             for(Student s: students){
                     if(targetYear == s.getYear()){
                             System.out.println(s);
                     }
             }
             System.out.print("\n-------Names of the Students in Insertion ordder-------\n ");
             Set<Student> linkedStudents = new LinkedHashSet<>();
             linkedStudents.addAll(students);

             for (Student s : linkedStudents) {
                 System.out.println(s.getName());
             }
        }
}

